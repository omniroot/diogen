{
  "name": "diogen",
  "private": true,
  "version": "0.2.0",
  "type": "module",
  "scripts": {
    "preinstall": "git submodule sync --recursive || true",
    "prepare-submodules": "git submodule update --init --recursive",
    "dev": "vite dev --port 5173 --host",
    "build:pre": "npm run prepare-submodules && npm run typegen-chakra",
    "build": "npm run build:pre && tsc -b && vite build",
    "biome:format": "yarn exec biome format --write ",
    "biome:lint": "yarn exec biome lint --write ",
    "preview": "vite preview --port 5173",
    "typegen-supabase": "npx supabase gen types typescript --project-id ddmwmjqgwvhbjvyifjzs --schema public > ./src/api/supabase.types.ts",
    "typegen-chakra": "chakra typegen ./src/theme/index.ts"
  },
  "dependencies": {
    "@chakra-ui/cli": "^3.26.0",
    "@chakra-ui/react": "^3.26.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/helpers": "^0.1.21",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/react": "^0.1.21",
    "@dnd-kit/sortable": "^10.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@siberiacancode/reactuse": "^0.2.22",
    "@supabase/supabase-js": "^2.50.2",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-query": "^5.81.5",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.122.0",
    "@tanstack/router-plugin": "^1.122.0",
    "@uidotdev/usehooks": "^2.4.1",
    "@vitejs/plugin-react-swc": "^4.0.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lightningcss": "^1.30.1",
    "magic-string": "^0.30.19",
    "next-themes": "^0.4.6",
    "react": "^19.1.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.59.0",
    "react-query-kit": "^3.3.1",
    "react-scan": "^0.4.3",
    "swapy": "^1.0.5",
    "unplugin-fonts": "^1.4.0",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/node": "^24.0.7",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "globals": "^16.2.0",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
