{
  "name": "diogen",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "git submodule sync --recursive || true",
    "prepare-submodules": "git submodule update --init --recursive",
    "dev": "vite dev --port 5173 --host",
    "preview": "vite preview --port 5173 --host",
    "build:pre": "npm run prepare-submodules && npm run typegen-chakra",
    "build": "npm run build:pre && tsc -b && vite build",
    "biome:format": "bunx biome format --write ./src/",
    "biome:lint": "bunx biome lint --write ./src/",
    "lf": "bun run biome:format && bun run biome:lint",
    "typegen": "bun run typegen:diogen && bun run typegen:kaizen && bun run typegen:chakra",
    "typegen:diogen": "npx supabase gen types typescript --project-id ddmwmjqgwvhbjvyifjzs --schema public > ./src/api/diogen.types.ts",
    "typegen:kaizen": "npx supabase gen types typescript --project-id yifheosdpnrhllqykpon --schema public > ./src/api/kaizen.types.ts",
    "typegen:appwrite": "bun appwrite pull tables && bun run appwrite types -l ts ./src/api/types/",
    "typegen:chakra": "chakra typegen ./src/theme/index.ts"
  },
  "dependencies": {
    "@chakra-ui/cli": "^3.27.0",
    "@chakra-ui/react": "^3.27.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/helpers": "^0.1.21",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/react": "^0.1.21",
    "@dnd-kit/sortable": "^10.0.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@emotion/react": "^11.14.0",
    "@emotion/server": "^11.11.0",
    "@ferrucc-io/emoji-picker": "^0.0.47",
    "@siberiacancode/reactuse": "^0.2.35",
    "@supabase/supabase-js": "^2.58.0",
    "@tabler/icons-react": "^3.34.1",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.132.37",
    "@tanstack/react-router-devtools": "^1.132.37",
    "@tanstack/router-plugin": "^1.132.37",
    "@uidotdev/usehooks": "^2.4.1",
    "appwrite": "^21.4.0",
    "appwrite-cli": "^12.0.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "emoji-mart": "^5.6.0",
    "emoji-picker-react": "^4.14.0",
    "frimousse": "^0.3.0",
    "lightningcss": "^1.30.2",
    "magic-string": "^0.30.19",
    "next-themes": "^0.4.6",
    "react": "^19.2.0",
    "react-calendar": "^6.0.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.64.0",
    "react-scan": "^0.4.3",
    "swapy": "^1.0.5",
    "unplugin-fonts": "^1.4.0",
    "vite-bundle-analyzer": "^1.2.3",
    "vite-bundle-visualizer": "^1.2.1",
    "vite-plugin-compression": "^0.5.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.5",
    "@types/node": "^24.6.2",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "@vitejs/plugin-react": "^5.0.4",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "globals": "^16.4.0",
    "typescript": "~5.9.3",
    "vite": "^7.1.9"
  }
}
